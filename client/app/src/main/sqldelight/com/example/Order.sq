CREATE TABLE Orders (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    dishId INTEGER NOT NULL,
    dishName TEXT NOT NULL,
    dishCost INTEGER NOT NULL,
    imageUrl TEXT
);

insertOrder:
INSERT INTO Orders (dishId, dishName, dishCost, imageUrl)
VALUES (?, ?, ?, ?);

selectAllOrders:
SELECT * FROM Orders;

deleteAllOrders:
DELETE FROM Orders;